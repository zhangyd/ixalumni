<%= form_for(@project) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br/><br/>

	<%= f.label :link %>
	<%= f.text_field :url %>
	<br/><br/>

	<%= f.label :description %>
	<%= f.text_field :description %>
	<br/><br/>

	<%= f.label :screenshot %>
	<%= f.file_field :screenshot %>
	<br/><br/>

	<%= f.submit %>
<% end %>